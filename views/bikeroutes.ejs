    <body>
        <h1>Welcome to find bike routes for you!</h1>
<button><a href="/">Back to station view!</a></button>


<form action="/getroutes" method="POST">
    <label for="gsearch"></label>
        <input type="search" name="gsearch" name="gsearch">
        <input type="submit" value="submit">
    </form>

    <%routesList.forEach(route => {%>
        <tr>
            
            <td><%= route.nimi %></td>
            <td><%= route.osoite %></td>
           
        </tr>
    <%})%>

   
   
   
   
   
   




        
       
       
       
                   

           
           
           
           
           
                
               
           
        
